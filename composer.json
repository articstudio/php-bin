{
    "name": "articstudio/php-bin",
    "type": "library",
    "description": "",
    "keywords": [
        "partio",
        "lingvo"
    ],
    "homepage": "https://github.com/articstudio/php-bin",
    "authors": [
        {
            "name": "Artic studio",
            "email": "developers@articstudio.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "jakub-onderka/php-console-highlighter": "^0.4.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "localheinz/composer-normalize": "^1.0",
        "php-school/cli-menu": "^3.0",
        "phpmetrics/phpmetrics": "^2.3",
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/console": "^4.2",
        "symfony/process": "^4.2"
    },
    "require-dev": {
        "illuminate/database": "5.7.*",
        "psr/log": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "subtree": {
            "Articstudio/git-test" : "https://github.com/articstudio/git-test.git"
        }
    },
    "extra": {},
    "autoload": {
        "psr-4": {
            "Articstudio\\PhpBin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Articstudio\\PhpBin\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.articstudio.com"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {},
    "support": {
        "issues": "https://github.com/articstudio/php-bin/issues",
        "source": "https://github.com/articstudio/php-bin"
    },
    "bin": [
        "bin/phpbin"
    ]
}